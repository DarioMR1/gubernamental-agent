  "eval_cases": [
    {
      "eval_id": "authentication_flow",
      "conversation": [
        {
          "invocation_id": "auth_test_1",
          "user_content": {
            "parts": [
              {
                "text": "Hola, necesito ayuda"
              }
            ],
            "role": "user"
          },
          "expected_final_response_contains": [
            "autenticar",
            "email",
            "contraseña"
          ]
        },
        {
          "invocation_id": "auth_test_2", 
          "user_content": {
            "parts": [
              {
                "text": "Mi email es usuario@test.com y mi contraseña es password123"
              }
            ],
            "role": "user"
          },
          "expected_final_response_contains": [
            "hola",
            "autenticado",
            "ine"
          ]
        }
      ]
    },
